{
  "$schema": "http://json.schemastore.org/template",
  "author": "Gary Woodfine",
  "name": "Database Project",
  "description": "Dotnet database project template for entity framework ",
  "identity": "Database Project",
  "shortName": "database",
  "sourceName": "DatabaseProject",
  "preferNameDirectory": "true",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "symbols": {
    "root": {
      "type": "parameter",
      "replaces": "placeHolder",
      "dataType": "string",
      "defaultValue": "Threenine"
    },
    "mssql": {
      "type": "computed",
      "value": "(!postgre)"
    },
    "postgre": {
      "type": "parameter",
      "datatype": "bool",
      "defaultValue": "false"
    }
  },
  "classifications": [
    "dotnet",
    "csharp",
    "EF",
    "Entity Framework"
  ]
}